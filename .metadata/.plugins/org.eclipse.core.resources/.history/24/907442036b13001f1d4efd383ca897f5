package com.demo.beans;

public class MyClass {
	public void Isprime(int n)
	{
		boolean flag=false;
		for(int i=2;i<Math.sqrt(n);i++)
		{
			if(n%i==0)
			{
				flag=true;
				break;
				
			}	
		}
		if(flag==true)
		{
			System.out.println("Not a Prime Number");
		}
		else
		{
			System.out.println(" Prime Number!!!");
		}
		
	}

}
